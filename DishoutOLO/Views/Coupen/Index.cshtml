@{
    ViewData["Title"] = "Coupen";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Coupen</h3>
                    <div class="col-lg-12  text-right">
                        <a href="/Coupen/Create" class="btn btn-md btn-warning">
                            Add Coupen
                        </a>
                    </div>
                </div>


                <!-- /.card-header -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="coupenTbl" class="table table-bordered table-hover">
                            <thead class="thead-light">
                                <tr>
                                    <th scope="col" class="sort" data-sort="name">Coupen Name</th>
                                    <th scope="col" class="sort" data-sort="name">Coupon Code</th>
                                    <th scope="col" class="sort" data-sort="name">Start Date</th>
                                    <th scope="col" class="sort" data-sort="name">End Date</th>
                                    <th scope="col" class="sort" data-sort="name">Discount</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<partial name="_CommonResponseModel" />

@section Scripts {

    <script src="~/Assets/Coupen.js " asp-append-version="true"></script>
}

















